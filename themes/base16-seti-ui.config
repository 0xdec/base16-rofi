rofi.color-active:  #8ec43d, #55b5db, #8ec43d, #8ec43d, #55b5db
rofi.color-urgent:  #8ec43d, #Cd3f45, #8ec43d, #8ec43d, #Cd3f45

! Set the desired separator style
rofi.separator-style: solid
